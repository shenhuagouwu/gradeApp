
view,text,input{
    box-sizing: border-box;
    font-family: Microsoft Yahei, Arial, Helvetica, sans-serif;
}
.canvas-img{
    width: 185rpx;
    height: 185rpx;
    position: absolute;
    left:50%;
    top:30%;
}
.canvas{
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.water-mark{
    width: 100%;
    height: 100%;
    position: absolute;
    background-repeat: repeat;
    background-position: left top;
    background-size: 185rpx 185rpx;
    left:0;
    top:0;
}
page{
    background: #f7f7f7;
}
.is-loading{
    width: 100%;
    height: 100vh;
    position: relative;
    overflow: hidden;
}
.is-loading .loading-mark{
    position: absolute;
    left:0;
    top:0;
    width: 100%;
    height: 100%;
    background: #f7f7f7;
    z-index: 10;
}

.combine-compare{
    width: 100%;
    overflow: hidden;
    position: relative;
    padding: 40rpx 30rpx 30rpx;
}
.combine-compare:before{
    content:'';
    display: block;
    width: 100%;
    height: 280rpx;
    background: #1e81d2;
    position: absolute;
    left:0;
    top:0;
}
.combine-compare .compare-panel{
    width: 100%;
    position: relative;
    z-index: 10;
    border-radius: 15rpx;
    background: linear-gradient(#fdfeff,#ffffff);
    padding: 36rpx 25rpx;
    overflow: hidden;
    box-shadow: 0 5rpx 5rpx rgba(30,129,210,.04);
}
.compare-title{
    width: 100%;
    padding: 0 20rpx;
    display: flex;
    overflow: hidden;
    position: relative;
    z-index: 10;
}
.compare-title.two-date .combine-title{
    height: 84rpx;
}
.compare-title .combine-title{
    height: 48rpx;
    font-size:0;
}
.compare-title.two-date .combine-title .txt-name{
    height: 84rpx;
    line-height: 84rpx;
}
.compare-title .combine-title .txt-name{
    display: inline-block;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 30rpx;
    color: #171d2b;
    font-weight: bold;
}
.compare-title.two-date .combine-title .txt-font{
    height:  84rpx;
    line-height: 84rpx;
}
.compare-title .combine-title .txt-font{
    font-size: 30rpx;
    height:  48rpx;
    line-height: 48rpx;
    color: #e81010;
    margin: 0 10rpx;
    font-family: Arial, Helvetica, sans-serif;
}
.compare-title.two-date .line{
    height: 84rpx;
}
.compare-title .line{
    flex: 1;
    height: 48rpx;
    overflow: hidden;
    position: relative;
}
.compare-title .line:before{
    content:'';
    width: 60%;
    height: 1px;
    background: #ececec;
    top: 50%;
    right: 12rpx;
    position: absolute;
}
.compare-title .date{
    height: 48rpx;
    border: 1px solid #ececec;
    background: #f5f5f5;
    padding: 5rpx 60rpx 5rpx 25rpx;
    font-size:0;
    border-radius: 48rpx;
    position: relative;
}
.compare-title.two-date .date{
    height: 84rpx;
    border: 1px solid #ececec;
    background: #f5f5f5;
    font-size:0;
    border-radius: 84rpx;
}
.compare-title .date .txt-date{
    display: block;
    height: 36rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #000000;
    overflow: hidden;
}
.compare-title .date .icon-arrow{
    width:0;
    height:0;
    display: inline-block;
    border-top: 10rpx solid #999999;
    border-left: 10rpx dashed transparent;
    border-right: 10rpx dashed transparent;
    border-bottom: none;
    position: absolute;
    right: 25rpx;
    top: 50%;
    margin-top: -5rpx;
}
.nav-panel{
    width: 100%;
    background: #fff;
    overflow: hidden;
    padding: 30rpx 0;
    display: flex;
    position: relative;
}
.nav-panel .fixed-height{
    width: 100%;
    position: absolute;
    height: 84rpx;
    left:0;
    top:0;
}
.nav-panel .item-nav{
    flex: 1;
    overflow: hidden;
    font-size:0;
    text-align: center;
    position: relative;
    z-index: 10;
}
.nav-panel .item-nav .txt-nav{
    display: inline-block;
    height: 54rpx;
    line-height: 54rpx;
    font-size: 30rpx;
    padding: 0 30rpx;
    color: #3c3c3c;
}
.nav-panel .item-nav.on .txt-nav{
    background: linear-gradient(to right,#f51611,#f65006);
    border-radius: 54rpx;
    color: #fff;
}
.fixed-nav.on{
    display: block;
}
.fixed-nav{
    width: 100%;
    background: #1e81d2;
    overflow: hidden;
    display: none;
    position: fixed;
    left:0;
    top:0;
    z-index: 1020;
}
.fixed-nav.is-two .item-nav{
    width: 50%;
}
.fixed-nav .item-nav{
    width: 33.3333%;
    float:left;
    overflow: hidden;
    font-size:0;
    text-align: center;
    padding: 15rpx 0;
    position: relative;
}
.fixed-nav .item-nav .txt-nav{
    display: inline-block;
    height: 54rpx;
    line-height: 54rpx;
    font-size: 30rpx;
    padding: 0 30rpx;
    color: #ffffff;
}
.fixed-nav .item-nav.on:before{
    content:'';
    display: block;
    position: absolute;
    width:0;
    height:0;
    bottom:0;
    left: 50%;
    margin-left: -12rpx;
    border-bottom: 12rpx solid #fff;
    border-left: 12rpx dashed transparent;
    border-right: 12rpx dashed transparent;
    border-top:none;
}

.footer{
    width: 100%;
    padding: 60rpx 50rpx 50rpx;
    overflow: hidden;
    background: #ffffff;
    margin-top: -10rpx;
}
.footer .txt-p{
    width: 100%;
    height: 54rpx;
    overflow: hidden;
    text-align: center;
    position: relative;
}
.footer .txt-p .txt-line{
    width: 100%;
    height: 27rpx;
    border-bottom: 1px solid #f1f6ff;
    position: absolute;
    left:0;
    top:0;
}
.footer .txt-p .txt-font{
    display: inline-block;
    height: 54rpx;
    line-height: 54rpx;
    font-size: 24rpx;
    color: #bcbcbc;
    padding: 0 20rpx;
    background: #ffffff;
    z-index: 2;
    position: relative;
}

.date-picker{
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 4080;
    left:0;
    top:0;
}
.date-picker .picker-mark{
